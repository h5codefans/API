package cn.bjsxt.collection;


public class SxLinkedList /*implements List*/ {
      
	private Node first;
	private Node last;
	
	private int size;
	
	public void add(Object obj){
		Node n=new Node();
		if(first==null){
			n.setPrevious(null);
			n.setObj(obj);
			n.setNext(null);
			
			first=n;
			last=n;
		}else{
			//直接往last节点后增加新的节点
			n.setPrevious(last);
			n.setObj(obj);
			n.setNext(null);
			
			last.setNext(n);
			
			last=n;
		}
		size++;
	}
	
	public int size(){
		return size;
	}
	
	public Object get(int index){
		Node temp=null;
		if(first!=null){
			 temp=first;
			for(int i=0;i<index;i++){
				temp=temp.next;
			}
		}
		
		//index 超过越界
		return temp;
	}
	public static void main(String[] args) {
		SxLinkedList list=new SxLinkedList();
		list.add("aaa");
		list.add("bbb");
		System.out.println(list.size());
		System.out.println(list.get(1));
	}
}
